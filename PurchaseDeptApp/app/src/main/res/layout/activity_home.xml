<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:id="@+id/scrollLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true"
        tools:context=".ActivityHome">

        <LinearLayout
            android:id="@+id/constraintLayout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="25dp"
            android:layout_marginEnd="25dp"
            android:orientation="vertical">

            <include
                android:id="@+id/layoutZippeee"
                layout="@layout/zippe_logo"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:layout_marginTop="20dp" />

            <Button
                android:id="@+id/cat_spices_masala_herbs"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/spices_masala_herbs" />


            <Button
                android:id="@+id/cat_flour_grains_pulses"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/flour_grains_pulses" />

            <Button
                android:id="@+id/cat_oil_ghee"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/oil_ghee" />

            <Button
                android:id="@+id/cat_sugar_sweetners"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/sugar_sweetners" />

            <Button
                android:id="@+id/cat_dairy"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/dairy" />

            <Button
                android:id="@+id/cat_snacks"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/snacks" />

            <Button
                android:id="@+id/cat_confectionary_dessert"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/confectionary_dessert" />

            <Button
                android:id="@+id/cat_breakfast_essentials_cereals"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/breakfast_essentials_cereals" />

            <Button
                android:id="@+id/cat_beverages"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/beverages" />

            <Button
                android:id="@+id/cat_premixes"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/premixes" />

            <Button
                android:id="@+id/cat_dry_fruit"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/dry_fruits" />

            <Button
                android:id="@+id/cat_cooking_paste"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/cooking_paste" />

            <Button
                android:id="@+id/cat_baking_essentials"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/baking_essentials" />

