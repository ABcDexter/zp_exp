                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/diapers_wipes" />

            <Button
                android:id="@+id/cat_female_hygiene"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/female_hygiene" />

            <Button
                android:id="@+id/cat_everyday_medicine_sexual_wellness"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/everyday_medicine_sexual_wellness" />

        </LinearLayout>
    </ScrollView>
</androidx.drawerlayout.widget.DrawerLayout>