                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/ready_meals_mixes" />

            <Button
                android:id="@+id/cat_packaged_essentials"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/packaged_essentials" />

            <Button
                android:id="@+id/cat_frozen_packaged_veg"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/frozen_packaged_veg" />

            <Button
                android:id="@+id/cat_frozen_packaged_non_Veg"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/frozen_packaged_non_Veg" />

            <Button
                android:id="@+id/cat_cleaners"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/cleaners" />

            <Button
                android:id="@+id/cat_paper_towels"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/paper_towels" />

            <Button
                android:id="@+id/cat_misc_household"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/misc_household" />

            <Button
                android:id="@+id/cat_skin_care"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/skin_care" />

            <Button
                android:id="@+id/cat_hair_care"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/hair_care" />

            <Button
                android:id="@+id/cat_oral_care"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/oral_care" />

            <Button
                android:id="@+id/cat_deos_perfumes"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/btn_bkg"
                android:text="@string/deos_perfumes" />

            <Button
